{
  "version": 2,
  "builds": [
    {
      "src": "redflag_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "redflag_project/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "redflag_project.settings"
  }
}
